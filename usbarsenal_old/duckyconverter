            ISO_8859_5_DA=$(construct_kbbyte $KEY_K)
            ISO_8859_5_BB=$(construct_kbbyte $KEY_L $MODIFIERKEY_SHIFT)
            ISO_8859_5_DB=$(construct_kbbyte $KEY_L)
            ISO_8859_5_CC=$(construct_kbbyte $KEY_SEMICOLON $MODIFIERKEY_SHIFT)
